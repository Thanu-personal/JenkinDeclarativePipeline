pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo "This is build step"
            }
        }
        stage('Test') { 
            steps {
                 echo "This is Test step"
            }
        }
        stage('Deploy') { 
            steps {
                sh src
                javac HelloWorld.java
                java HellWorld
                 echo "This is Deploy step"
            }
        }
    }
}
